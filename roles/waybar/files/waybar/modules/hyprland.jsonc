{
  "hyprland/workspaces": {
    "format": "{name}"
  },
  "hyprland/window": {
    "format": "{initialClass}",
    "icon": true,
    "icon-size": 18,
    "seperate-outputs": true,
    "rewrite": {
      "": "з░Бе Gentoo",
      "firefox-esr": "Firefox",
      "jetbrains-studio": "Android Studio",
      "(.*)obsidian(.*)": "Obsidian",
      "(.*)telegram(.*)": "Telegram",
      "wechat": "WeChat"
    },
    "on-click-middle": "pkill waybar && hyprctl dispatch exec waybar"
  }
}
